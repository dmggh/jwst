#! /usr/bin/env pysh
#-*-python-*-

usage("N [words...]", 1, 10**9, help="Print N-1 of every N words")

def decimate(interval, words):
    result = []
    for i, word in enumerate(words):
        if not (i % interval == 0):
            result.append(word)
    return result

to_be_deleted = decimate(int(sys.argv[1]), sys.argv[2:])

for word in to_be_deleted:
    print(word)


